---
dns:
  ipv4: yes
  ipv6: no
  proxied: no
docker:
  json_driver: no
gpu:
  intel: yes
  nvidia: no
mounts:
  ipv4_only: no
system:
  timezone: auto
traefik:
  cert:
    http_validation: no
    zerossl: no
  error_pages: no
  hsts: no
  metrics: no
  provider: cloudflare
  subdomains:
    dash: dash
    jaeger: jaeger
    metrics: metrics
  tracing: no
